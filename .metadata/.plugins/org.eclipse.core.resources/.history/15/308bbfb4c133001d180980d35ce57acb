
public class Porco {

	private String nome;
	private SexoPorcoEnum sexo;
	private byte idade;
	private short peso;

	public Porco(String nome, SexoPorcoEnum sexo, byte idade, short peso){
		this.nome=nome;
		this.sexo=sexo;
		this.idade=idade;
		this.peso=peso;
	}

	public String getNome() {
		return nome;
	}

	public SexoPorcoEnum getSexo() {
		return sexo;
	}

	public byte getIdade() {
		return idade;
	}

	public short getPeso() {
		return peso;
	}
	
	
	
}
