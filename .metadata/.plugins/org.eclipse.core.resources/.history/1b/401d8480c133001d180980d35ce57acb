import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		ArrayList<Porcos> porco= new ArrayList<>();

		for (Porcos porcos : porco) {
		     System.out.println("Nome "+porcos.nome+", sexo:  "+porcos.sexo+ "idade: "+porcos.idade+ "peso"+ porcos.peso);

		}
	}
}
